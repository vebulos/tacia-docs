{
  "name": "tacia-docs",
  "type": "module",
  "version": "1.0.0",
  "description": "Tacia Documentation Site",
  "scripts": {
    "start": "npm run dev",
    "dev": "npx ng serve --open --port=4200",
    "build": "npx ng build --configuration=production",
    "build:dev": "npx ng build --configuration=development",
    "watch": "npx ng build --watch --configuration development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "npx ng lint",
    "serve:static": "npm run build && npx http-server dist/frontend/browser -p 8080"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "localforage": "^1.10.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.19.0",
    "@angular-devkit/build-angular": "^19.2.8",
    "@angular/cli": "^19.2.8",
    "@angular/compiler-cli": "^19.2.0",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/angular": "^17.4.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/highlight.js": "^9.12.4",
    "@types/node": "^20.19.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.7.2",
    "vitest": "^3.2.4"
  }
}
